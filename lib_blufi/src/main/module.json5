{
  "module": {
    "name": "lib_blufi",
    "type": "shared",
    "description": "$string:shared_desc",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "pages": "$profile:main_pages",
    "requestPermissions": [
      {
        "name": "ohos.permission.ACCESS_BLUETOOTH",
        "reason": "$string:permission_bluetooth_reason",
        "usedScene": {
          "when": "inuse"
        }
      }
    ]
  }
}